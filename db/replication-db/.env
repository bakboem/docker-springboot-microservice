# Basic PostgreSQL settings
POSTGRES_DB=codera                     # Database name (数据库名)
POSTGRES_USER=codera-user              # Replication user (复制用户)
POSTGRES_PASSWORD=codera12345!         # Password for replication user (复制用户密码)

# Replication configuration
MASTER_HOST=main-db                    # Master DB hostname or IP (主库的主机名或IP地址)
MASTER_PORT=5432                       # Master DB port (主库的端口)
